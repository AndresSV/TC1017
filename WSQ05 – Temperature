#include <iostream>
using namespace std;

int main(){
  double inA = 0;
  double celsius = 0;
  cout << "Insert the value of Farenheit to convert to Celsius. " << endl;
  cin >> inA;
  celsius = (((inA-32)/9)*5);
  cout << "Celsius value is: " << celsius << endl;
  if(celsius <= 0){
    cout << "Water is usually frozen at this temperature." << endl;
  }
  else if(celsisus <=0 && celsius >100){
    cout << "Water is usually liquid at this temperature." << endl;
  }
  else{
    cout << "Water is usually vapor at this temperature." << endl;
  }
  return 0;
}
